        android:label="fragment_chats"
        tools:layout="@layout/fragment_chats" />
    <fragment
        android:id="@+id/catalogo"
        android:name="com.kiwistudio.spelltrader.ui.main.Catalogo"
        android:label="fragment_catalogo"
        tools:layout="@layout/fragment_catalogo" />
    <fragment
        android:id="@+id/ajustes"
        android:name="com.kiwistudio.spelltrader.ui.main.Ajustes"
